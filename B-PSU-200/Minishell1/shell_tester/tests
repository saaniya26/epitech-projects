[1]
NAME="Nothing"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo ''
[1-END]

[2.1]
NAME="Exec command"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls /etc'
[2.1-END]

[2.2]
NAME="Exec two command"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'ls /etc'
  echo 'cat /etc/resolv.conf'
[2.2-END]

[2.3]
NAME="Exec five commands"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cat /etc/resolv.conf'
  sleep 0.5
  echo 'ls -l /etc'
  sleep 0.5
  echo 'pwd'
[2.3-END]

[3.1]
NAME="Testing with /usr/bin PATH"
SETUP="export TERM=xterm ; PATH='/usr/bin'"
CLEAN=""
TESTS=
  echo 'pwd'
[3.1-END]

[3.2]
NAME="Testing with /bin PATH"
SETUP="export TERM=xterm ; PATH='/bin'"
CLEAN=""
TESTS=
  echo 'pwd'
[3.2-END]

[3.3]
NAME="Testing with wrong PATH"
SETUP="export TERM=xterm ; PATH=''"
CLEAN=""
TESTS=
  echo 'pwd'
[3.3-END]

[3.4]
NAME="Testing with wrong PATH 2"
SETUP="export TERM=xterm ; PATH='/'"
CLEAN=""
TESTS=
  echo 'pwd'
[3.4-END]

[4]
NAME="setenv and unsetenv"
SETUP="/usr/bin/env > .lstenv ; while read varenv ; do unset $varenv ; done < <(/bin/cat .lstenv | /usr/bin/cut -f1 -d=) ; export PATH=/bin:/usr/bin ; export _=ls ; export LS_COLORS=RID ; export MYBG=4 ; export LVL=4 ; export NOTE=2"
CLEAN="while read varenv ; do export "$varenv" 2>/dev/null ; done  < <(/bin/cat .lstenv) ; /bin/rm -f .lstenv"
TESTS=
  echo 'unsetenv HOSTTYPE'
  echo 'unsetenv VENDOR'
  echo 'unsetenv OSTYPE'
  echo 'unsetenv MACHTYPE'
  echo 'unsetenv SHLVL'
  echo 'unsetenv PWD'
  echo 'unsetenv LOGNAME'
  echo 'unsetenv USER'
  echo 'unsetenv GROUP'
  echo 'unsetenv HOST'
  echo 'unsetenv REMOTEHOST'
  echo 'unsetenv _'
  echo 'unsetenv LS_COLORS'
  echo 'unsetenv NOTE'
  echo 'setenv titi'
  echo 'setenv tata coucou'
  echo 'env'
  echo '/usr/bin/env'
  echo 'setenv var1 val1'
  echo 'setenv var2 val2'
[4-END]

[5.1]
NAME="builtin cd (Basic)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cd ..'
  echo 'ls -l'
  echo 'cd /usr'
  echo 'ls -l'
[5.1-END]

[5.2]
NAME="builtin cd (Go OLDPWD)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cd ..'
  echo 'pwd'
  echo 'cd /usr'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
[5.2-END]

[5.3.1]
NAME="builtin cd (Go OLDPWD [Advanced 1])"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo 'cd ..'
  echo 'pwd'
  echo 'cd /usr'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  unsetenv OLDPWD
  echo 'cd -'
  echo 'pwd'
[5.3.1-END]

[5.3.2]
NAME="builtin cd (Go OLDPWD [Advanced 2])"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  unsetenv PWD
  unsetenv OLDPWD
  echo 'cd ..'
  echo 'pwd'
  unsetenv PWD
  unsetenv OLDPWD
  echo 'cd /usr'
  echo 'pwd'
  unsetenv PWD
  unsetenv OLDPWD
  echo 'cd -'
  echo 'pwd'
  unsetenv PWD
  unsetenv OLDPWD
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
[5.3.2-END]

[5.4]
NAME="builtin cd (Advanced)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  unsetenv PWD
  unsetenv OLDPWD
  echo 'pwd'
  echo 'cd ..'
  echo 'pwd'
  echo 'cd /usr'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
[5.4-END]

[5.5]
NAME="builtin cd (Advanced Continued)"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  unsetenv PWD
  unsetenv OLDPWD
  echo 'pwd'
  echo 'cd ..'
  echo 'pwd'
  echo 'cd /usr'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd -'
  echo 'pwd'
  echo 'cd ..'
  echo 'pwd'
  echo 'cd /usr'
  echo 'pwd'
[5.5-END]

[6]
NAME="many tab and space"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo -e "\t\t    ls\t     \t-l    \t\t  \t -a /usr   \t\t"
[6-END]

[7.1]
NAME="Launch DivZero program with core dump"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "./my_divzero"
[7.1-END]

[7.2]
NAME="Launch Segfault program with core dump"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo "./my_segfault"
[7.2-END]

[8.1]
