NAME="(Official) Line formatting (space and tabs) 2"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo -e "\t\t    echo\t     \t-l    \t\t hello \t -a world   \t\t"
[Official_5.2-END]
