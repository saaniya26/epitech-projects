[Official_5.1]
NAME="(Official) Line formatting (space and tabs) 1"
SETUP="export TERM=xterm ; PATH='/bin:/usr/bin'"
CLEAN=""
TESTS=
  echo -e "\t\t    ls\t     \t-l    \t\t  \t -a /usr   \t\t"
[Official_5.1-END]

